{
  "name": "extra-fetch",
  "version": "2.0.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "browser": {
    "./lib/es2015/fetch.js": "./lib/es2015/fetch.browser.js",
    "./lib/es2018/fetch.js": "./lib/es2018/fetch.browser.js",
    "./lib/es2015/headers.js": "./lib/es2015/headers.browser.js",
    "./lib/es2018/headers.js": "./lib/es2018/headers.browser.js",
    "./lib/es2015/request.js": "./lib/es2015/request.browser.js",
    "./lib/es2018/request.js": "./lib/es2018/request.browser.js",
    "./lib/es2015/response.js": "./lib/es2015/response.browser.js",
    "./lib/es2018/response.js": "./lib/es2018/response.browser.js",
    "./lib/es2015/form-data.js": "./lib/es2015/form-data.browser.js",
    "./lib/es2018/form-data.js": "./lib/es2018/form-data.browser.js",
    "./lib/es2015/abort-controller.js": "./lib/es2015/abort-controller.browser.js",
    "./lib/es2018/abort-controller.js": "./lib/es2018/abort-controller.browser.js",
    "./lib/es2015/blob.js": "./lib/es2015/blob.browser.js",
    "./lib/es2018/blob.js": "./lib/es2018/blob.browser.js",
    "./lib/es2015/event-source.js": "./lib/es2015/event-source.browser.js",
    "./lib/es2018/event-source.js": "./lib/es2018/event-source.browser.js"
  },
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-fetch.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.2.1",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/eventsource": "^1.1.8",
    "@types/jest": "^27.4.0",
    "@types/lodash.frompairs": "^4.0.6",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "husky": "4",
    "jest": "^27.4.7",
    "micri": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@blackglory/types": "^0.6.5",
    "chalk": "^4.1.2",
    "eventsource": "^1.1.0",
    "extra-abort": "^0.1.1",
    "extra-logger": "^0.5.5",
    "iterable-operator": "^0.14.5",
    "justypes": "^0.1.6",
    "lodash.frompairs": "^4.0.1",
    "return-style": "^0.12.5",
    "undici": "^4.13.0",
    "value-getter": "^0.2.2"
  }
}
